
<p id="notice"><%= notice %></p>

<div class="container">
<h1 class="title-reg is-centered">Members List</h1>
  <div class="row mx-auto">
  <% @users.each do |user| %>
  <div class="card border-success mx-4 col-lg-3 col-md-6 col-12">
